{
    "SchemaDefinition": "ComputerSystem.v1_0_0",
    "ProfileName": "ComputerSystem",
    "ProfileVersion": "1.0.0",
    "Purpose": "This is a sample Redfish Interoperability profile.",
    "OwningEntity": "Dell",
    "ContactInfo": "abc@dell.com",
    "Resources": {
	"ComputerSystemCollection": {
		"PropertyRequirements": {
			"Members": {
				"MinCount": 1
			}
		}
	},
        "ComputerSystem": {
            "MinVersion": "1.0.0",
            "Purpose": "Every implementation must have the logical view of the System.",
            "PropertyRequirements": {
                "Id": {
                    "ReadRequirement": "Recommended"
                },
                "UUID": {
                    "ReadRequirement": "Recommended"
                },
                "SerialNumber": {
                    "ReadRequirement": "Recommended"
                },
                "Manufacturer": {
                    "ReadRequirement": "Recommended"
                },
                "Model": {
                    "ReadRequirement": "Recommended"
                },
                "MemorySummary": {
                    "ReadRequirement": "Recommended"
                },
                "SKU": {
                    "ReadRequirement": "Recommended"
                },
                "BiosVersion": {
                    "ReadRequirement": "Recommended"
                },
                "HostName": {
                    "ReadRequirement": "Recommended"
                },
                "PowerState":{ 
                        "Comparison": "AnyOf",
                        "Values": ["On","Off","PoweringOn","PoweringOff"]
		},
		"IndicatorLED":{
			"Comparison": "AnyOf",
			"Values": ["Lit", "Blinking", "Off", "Unknown"]
		},
		"SystemType":{
			"Comparison": "AnyOf",
			"Values": [
				"Physical", "Virtual", "OS", 
				"PhysicallyPartitioned", 
				"VirtuallyPartitioned", "Composed"
			]
		},
		"Boot": {
			"PropertyRequirements": {
				"BootSourceOverrideEnabled": {
					"Comparison": "AnyOf",
                                        "Values": ["Once","Continuous","Disabled"]
				},
				"BootSourceOverrideMode": {
					"Comparison": "AnyOf",
                                        "Values": ["UEFI","Legacy"]
				},
				"BootSourceOverrideTarget": {
					"MinSupportValues": [
						"None", "Pxe", "Floppy", "Cd", 
						"Hdd", "BiosSetup", "Utilities", 
						"UefiTarget", "SDCard", "UefiHttp"
					]
				}
			}
		},
                "Links": {
			"PropertyRequirements":{
				"ManagedBy": {}
			}
		}
            },
            "ActionRequirements":{
               "Reset": {
                    "ReadRequirement": "Mandatory",
                    "Purpose": "Ability to reset the system is a core requirement of most users.",
                    "Parameters": {
                        "ResetType": {
                            "ParameterValues": [
				"On", "ForceOff", "ForceRestart", 
				"GracefulShutdown", "PushPowerButton", 
				"Nmi", "PowerCycle"
				],
                            "RecommendedValues": ["GracefulRestart"]
                        }
                    }
                }
		
            }
        }
    }
}

